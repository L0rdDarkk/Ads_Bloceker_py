{
    "manifest_version": 2,
    "name": "Dark Blocker by L0rdDarkk",
    "description": "Ad Blocker for all Albanian Websites",
    "permissions": ["activeTab", "nativeMessaging"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "48": "icon.png"
    },
    "externally_connectable": {
        "matches": ["<all_urls>"]
    },
    "applications": {
        "gecko": {
            "id": "albania_ads_blocker@example.com",
            "strict_min_version": "45.0"
        }
    }
}
